{
  "name": "nj-codes",
  "version": "0.1.0",
  "description": "My website and blog",
  "main": "public/main.js",
  "scripts": {
    "copy": "rrun tasks/copy",
    "css": "rrun tasks/css",
    "watch": "node tasks/watch.js",
    "js:dev": "webpack --env development -w",
    "js:client": "webpack --config webpack-client.js --env production -p",
    "js:server": "webpack --config webpack-server.js --env production",
    "js": "run-p js:client js:server",
    "server:dev": "nodemon -w dist/server.js dist/server.js",
    "server": "node dist/server.js",
    "dev": "npm run copy && run-p js:dev css watch server:dev",
    "build": "run-p copy css js",
    "start": "node dist/server.js",
    "deploy": "now --dotenv"
  },
  "author": "Nathan Jacobs",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "brotli-webpack-plugin": "^0.4.1",
    "chalk": "^2.1.0",
    "chokidar": "^1.7.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "postcss": "^6.0.12",
    "rrun": "^1.0.4",
    "shelljs": "^0.7.8",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "dependencies": {
    "apicache": "^1.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "express-static-gzip": "^0.3.0",
    "front-matter": "^2.3.0",
    "graphql-request": "^1.4.0",
    "helmet": "^3.8.2",
    "node-fetch": "^1.7.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-markdown": "^2.5.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "webpack": "^3.8.1",
    "wicg-focus-ring": "^2.0.2"
  }
