{
  "name": "nj-codes",
  "version": "0.1.0",
  "description": "Website of Nathan Jacobs",
  "main": "public/main.js",
  "scripts": {
    "copy": "node tasks/copy.js",
    "css": "node -e \"require('./tasks/sass')()\"",
    "css:watch": "node tasks/watch.js",
    "js": "webpack",
    "js:watch": "webpack -w",
    "server": "node dist/server.js",
    "server:watch": "nodemon -w dist/server.js -w dist/js dist/server.js",
    "dev": "npm run copy && run-p js:watch css:watch server:watch",
    "start": "node -r @std/esm server.js"
  },
  "author": "Nathan Jacobs",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "brotli-webpack-plugin": "^0.4.1",
    "chalk": "^2.1.0",
    "chokidar": "^1.7.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "postcss": "^6.0.12",
    "shelljs": "^0.7.8",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "@std/esm": "^0.11.1",
    "express": "^4.16.1",
    "express-static-gzip": "^0.3.0",
    "helmet": "^3.8.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "@std/esm": {
    "esm": "all"
  }
}
